import axios from "axios";

export function getUsers() {return async (dispatch) => {
    const response = await axios.get("https://localhost:3001/api/users");
    const data = await response.data;
    dispatch({
      type: "GET_USERS",
      payload: data,
    });
  };


  export function getUser(id) {return async (dispatch) => {
    const response = await axios.get(`https://localhost:3001/api/users/${id}`);
    const data = await response.data;
    dispatch({
      type: "GET_USER",
      payload: data,
    });
  };